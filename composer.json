{
    "name": "prograde/prograde-oort",
    "description": "Unified Webhook & Automation Engine",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Antigravity"
        }
    ],
    "require": {
        "php": ">=8.2",
        "woocommerce/action-scheduler": "^3.9",
        "monolog/monolog": "^3.9",
        "abraham/twitteroauth": "^8.1",
        "chubes4/ai-http-client": "^2.0.10",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "softcreatr/jsonpath": "^0.9",
        "mtdowling/jmespath.php": "^2.8",
        "symfony/expression-language": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/uid": "^7.2",
        "twig/twig": "^3.14",
        "league/oauth2-client": "^2.7",
        "league/flysystem": "^3.29",
        "league/csv": "^9.16",
        "league/uri": "^7.4",
        "ramsey/uuid": "^4.7",
        "nesbot/carbon": "^3.8",
        "respect/validation": "^2.3",
        "nyholm/psr7": "^1.8",
        "php-http/discovery": "^1.19",
        "firebase/php-jwt": "^6.10",
        "spatie/array-to-xml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^2.0",
        "php-stubs/wordpress-stubs": "^6.9",
        "wp-phpunit/wp-phpunit": "^6.9",
        "squizlabs/php_codesniffer": "^3.11",
        "wp-coding-standards/wpcs": "^3.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/extension-installer": "^1.4",
        "szepeviktor/phpstan-wordpress": "^2.0",
        "rector/rector": "^2.0",
        "fakerphp/faker": "^1.24",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^7.2",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "ProgradeOort\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
